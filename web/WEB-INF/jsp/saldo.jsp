<div class="card text-dark bg-light mb-3">
    <div class="card-header d-flex justify-content-between">
        <label>Perputaran Saldo Kas</label>
        <label><i class="bi bi-calendar2-week"></i> ${tanggal}</label>
    </div>
    <div class="custom-card text-white m-2">
        <div class="card-body rounded bg-success">
            <h5 class="card-title">Jumlah Kas Masuk</h5>
            <h6 class="card-text"><i class="bi bi-graph-up-arrow me-2"></i> Rp. ${totalKasM}</h6>
        </div>
        <div class="card-body rounded bg-danger">
            <h5 class="card-title">Jumlah Kas Keluar</h5>
            <h6 class="card-text"><i class="bi bi-graph-down-arrow me-2"></i> Rp. ${totalKasK}</h6>
        </div>
        <div class="card-body rounded bg-info">
            <h5 class="card-title">Total Saldo Kas</h5>
            <h6 class="card-text"><i class="bi bi-tag"></i> Rp. ${totalSaldo}</h6>
        </div>
    </div>
</div>